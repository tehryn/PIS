		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/admin/user_edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/admin/user_new.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>insert</from-outcome>
			<to-view-id>/admin/user_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/admin/user_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>admin/user_edit.xhtml</display-name>
		<from-view-id>/admin/user_edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>update</from-outcome>
			<to-view-id>/admin/user_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/admin/user_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/admin/service_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>/admin/service_new.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>delete</from-outcome>
			<to-view-id>/admin/service_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/admin/service_edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/admin/service_new.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>insert</from-outcome>
			<to-view-id>/admin/service_list.xhtml</to-view-id>
