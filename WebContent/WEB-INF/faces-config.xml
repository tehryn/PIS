<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
	version="2.3">
	<navigation-rule>
	    <from-view-id>/index.xhtml</from-view-id>
	    <navigation-case>
		    <from-outcome>hotel_info</from-outcome>
		    <to-view-id>/guest/hotel_info.xhtml</to-view-id>
		    <redirect />
	    </navigation-case>
	    <navigation-case>
		    <from-outcome>login</from-outcome>
		    <to-view-id>/guest/login.xhtml</to-view-id>
		    <redirect />
	    </navigation-case>
	    <navigation-case>
		    <from-outcome>register</from-outcome>
		    <to-view-id>/guest/registration.xhtml</to-view-id>
		    <redirect />
	    </navigation-case>
	    <navigation-case>
		    <from-outcome>admin</from-outcome>
		    <to-view-id>/admin/index.xhtml</to-view-id>
		    <redirect />
	    </navigation-case>
    </navigation-rule>
    <navigation-rule>
	    <from-view-id>/guest/offered_rooms.xhtml</from-view-id>
	    <navigation-case>
		    <from-outcome>index</from-outcome>
		    <to-view-id>/index.xhtml</to-view-id>
		    <redirect />
	    </navigation-case>
    </navigation-rule>
    <navigation-rule>
	    <from-view-id>/guest/offered_services.xhtml</from-view-id>
	    <navigation-case>
		    <from-outcome>index</from-outcome>
		    <to-view-id>/index.xhtml</to-view-id>
		    <redirect />
	    </navigation-case>
    </navigation-rule>
    <navigation-rule>
	    <from-view-id>/guest/registration.xhtml</from-view-id>
	    <navigation-case>
		    <from-outcome>index</from-outcome>
		    <to-view-id>/index.xhtml</to-view-id>
		    <redirect />
	    </navigation-case>
    </navigation-rule>
    <navigation-rule>
	    <from-view-id>/guest/login.xhtml</from-view-id>
	    <navigation-case>
		    <from-outcome>index</from-outcome>
		    <to-view-id>/index.xhtml</to-view-id>
		    <redirect />
	    </navigation-case>
    </navigation-rule>
    <navigation-rule>
	    <from-view-id>/guest/hotel_info.xhtml</from-view-id>
	    <navigation-case>
		    <from-outcome>index</from-outcome>
		    <to-view-id>/index.xhtml</to-view-id>
		    <redirect />
	    </navigation-case>
    </navigation-rule>
    <navigation-rule>
	    <from-view-id>/user/reservations.xhtml</from-view-id>
	    <navigation-case>
		    <from-outcome>index</from-outcome>
		    <to-view-id>/index.xhtml</to-view-id>
			<redirect />
	    </navigation-case>
	    <navigation-case>
		    <from-outcome>login</from-outcome>
		    <to-view-id>/guest/login.xhtml</to-view-id>
			<redirect />
	    </navigation-case>
    </navigation-rule>

	<navigation-rule>
		<from-view-id>/admin/room_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>/admin/room_new.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>delete</from-outcome>
			<to-view-id>/admin/room_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>edit</from-outcome>
			<to-view-id>/admin/room_edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/admin/room_new.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>insert</from-outcome>
			<to-view-id>/admin/room_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/admin/room_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>admin/room_edit.xhtml</display-name>
		<from-view-id>/admin/room_edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>update</from-outcome>
			<to-view-id>/admin/room_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/admin/room_list.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/admin/user_list.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>new</from-outcome>
